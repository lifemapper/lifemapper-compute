; Lifemapper configuration file for lmCompute

[contact]
INSTITUTION_NAME: Lifemapper job test
ADMIN_NAME: Test Admin
ADMIN_EMAIL: test@test.com
LOCAL_MACHINE_ID: 

[environment]
PLUGINS_DIR: @LMHOME@/lm/code/plugins 
APP_PATH: @LMHOME@/lm/apps/
JOB_DATA_PATH: @LMDISK@/tests/data/ 
JOB_OUTPUT_PATH: @LMDISK@/tests/jobs/ 
LM_USER_JOB: @LMUSER@

Sample Jobs Path: @LMDISK@/tests/sampleJobs
Sample Layers Path: @LMDISK@/tests/data/layers/testLayers.txt

[commands]
GDALINFO_CMD: @LMHOME@/bin/gdalinfo

; Plugins
[openModeller]
DEFAULT_LOG_LEVEL: debug
OM_MODEL_CMD: om_model
OM_PROJECT_CMD: om_project
OM_VERSION: 1.3.0

[maxent]
JAVA_CMD: java -cp
ME_CMD: maxent.jar
MDL_TOOL: density.MaxEnt
PRJ_TOOL: density.Project
ME_VERSION: 3.3.3k

[options]
Store Log Files: True
Log Storage Location: @LMDISK@/tests/jobs
