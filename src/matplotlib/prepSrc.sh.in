#!/bin/bash

# Purpose: Fetch @ARCHIVENAME@ wheel

# cycler 0.10.0
CYCLER_URL_DEP=https://files.pythonhosted.org/packages/f7/d2/e07d3ebb2bd7af696440ce7e754c59dd546ffe1bbe732c8ab68b9c834e61
# CYCLER_FILE_DEP=cycler-0.10.0-py2.py3-none-any.whl
CYCLER_DL=$CYCLER_URL_DEP/@CYCLER_FILE_DEP@

# dateutil 2.8.1
DATEUTIL_URL_DEP=https://files.pythonhosted.org/packages/d4/70/d60450c3dd48ef87586924207ae8907090de0b306af2bce5d134d78615cb
# DATEUTIL_FILE_DEP=python_dateutil-2.8.1-py2.py3-none-any.whl
DATEUTIL_DL=$DATEUTIL_URL_DEP/@DATEUTIL_FILE_DEP@

# kiwisolver 1.2.0
KIWI_URL_DEP=https://files.pythonhosted.org/packages/ae/23/147de658aabbf968324551ea22c0c13a00284c4ef49a77002e91f79657b7
# KIWI_FILE_DEP=kiwisolver-1.2.0-cp36-cp36m-manylinux1_x86_64.whl
KIWI_DL=$KIWI_URL_DEP/@KIWI_FILE_DEP@

# pyparsing 2.4.7
PYPARSING_URL_DEP=https://files.pythonhosted.org/packages/8a/bb/488841f56197b13700afd5658fc279a2025a39e22449b7cf29864669b15d
# PYPARSING_FILE_DEP=pyparsing-2.4.7-py2.py3-none-any.whl
PYPARSING_DL=$PYPARSING_URL_DEP/@PYPARSING_FILE_DEP@


DL_URL=https://files.pythonhosted.org/packages/93/4b/52da6b1523d5139d04e02d9e26ceda6146b48f2a4e5d2abfdf1c7bac8c40
DL_WHL=@ARCHIVENAME@-@VERSION@-cp36-cp36m-manylinux1_x86_64.whl

get_wheel () {
  echo "Fetch @ARCHIVENAME@ wheel file"
  wget "$DL_URL/$DL_WHL"
}

get_dependencies () {
  echo "Fetch cycler 0.10.0 dependency"
  wget "$CYCLER_DL"
  echo "Fetch dateutil 2.8.1 dependency"
  wget "$DATEUTIL_DL"
  echo "Fetch kiwisolver 1.2.0 dependency"
  wget "$KIWI_DL"
  echo "Fetch pyparsing 2.4.7 dependency"
  wget "$PYPARSING_DL"
}

### main ###
get_dependencies
get_wheel
